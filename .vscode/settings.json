{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_style": "file",
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.generator": "Unix Makefiles",
    "files.associations": {
        "*.tpp": "cpp",
        "*.hpp": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__node_handle": "cpp",
        "complex": "cpp",
        "deque": "cpp",
        "iterator": "cpp",
        "ios": "cpp",
        "istream": "cpp",
        "new": "cpp",
        "optional": "cpp",
        "queue": "cpp",
        "sstream": "cpp",
        "stack": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "variant": "cpp",
        "vector": "cpp",
        "initializer_list": "cpp",
        "any": "cpp",
        "array": "cpp",
        "memory": "cpp"
    }
}
